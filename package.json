{
  "name": "aws-lambda-canary",
  "version": "1.0.0",
  "description": "AWS lambda canary project",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "s3-deploy": "gulp deploy",
    "nocov": "node ./node_modules/.bin/mocha --compilers js:babel-register test/index.test.js",
    "test": "npm run lint && npm run coverage",
    "coverage": "./node_modules/.bin/babel-node ./node_modules/.bin/babel-istanbul cover ./node_modules/.bin/_mocha test/index.test.js --report lcovonly -- -R spec",
    "test-watch": "./node_modules/.bin/nodemon node_modules/lab/bin/lab -cv -I \"__core-js_shared__\"",
    "lint": "eslint --fix *.js **/*.js **/test/**/*.js",
    "lint-fix": "eslint --fix *.js **/*.js  **/schema/**/*.js **/schema/*.js **/test/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/numo-labs/aws-lambda-canary.git"
  },
  "author": "Jimmy Ruts",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/numo-labs/aws-lambda-canary/issues"
  },
  "homepage": "https://github.com/numo-labs/aws-lambda-canary",
  "devDependencies": {
    "archiver": "^0.21.0",
    "aws-sdk": "^2.2.29",
    "babel-cli": "^6.4.5",
    "babel-istanbul": "^0.6.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-plugin-transform-runtime": "^6.4.3",
    "babel-register": "^6.4.3",
    "eslint": "^1.10.3",
    "eslint-config-semistandard": "^5.0.0",
    "eslint-config-standard": "^4.4.0",
    "eslint-plugin-standard": "^1.3.1",
    "gulp": "^3.9.0",
    "gulp-babel": "^6.1.2",
    "gulp-install": "^0.6.0",
    "gulp-zip": "^3.0.2",
    "mocha": "^2.4.5",
    "nodemon": "^1.8.1",
    "run-sequence": "^1.1.5"
  },
  "includeFilesS3": [
    "index.js"
  ],
  "dependencies": {
    "babel-runtime": "^6.3.19",
    "request": "^2.67.0",
    "starwars": "^1.0.0"
  }
}
